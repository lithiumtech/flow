<ruleset comparisonMethod="maven"
		 xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0
		  http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">

	<ignoreVersions>
		<ignoreVersion type="regex">(?i).*alpha.*</ignoreVersion>
		<ignoreVersion type="regex">(?i).*beta.*</ignoreVersion>
		<ignoreVersion type="regex">(?i).*ce</ignoreVersion>
		<ignoreVersion type="regex">(?i).*ccs</ignoreVersion>
		<ignoreVersion type="regex">(?i).*ea</ignoreVersion>
		<ignoreVersion type="regex">(?i).*rc[\d\-].*</ignoreVersion>
		<ignoreVersion type="regex">(?i).*-b[0-9\.]*</ignoreVersion>
		<ignoreVersion type="regex">(?i).*-m[0-9]*</ignoreVersion>
		<ignoreVersion type="regex">(?i).*-cp[0-9]*</ignoreVersion>
		<ignoreVersion type="regex">(?i).*.pr[0-9]*</ignoreVersion>
		<ignoreVersion type="regex">.*-snapshot[0-9]*</ignoreVersion>
		<ignoreVersion type="regex">.*-atlassian.*</ignoreVersion>
		<ignoreVersion type="regex">.*-cloudera.*</ignoreVersion>
		<ignoreVersion type="regex">.*-kohsuke.*</ignoreVersion>
		<ignoreVersion type="regex">^20.*[0-9]{6}</ignoreVersion>
	</ignoreVersions>

	<rules>
		<rule groupId="org.eclipse.jetty">
			<ignoreVersions>
				<ignoreVersion type="regex">1.*</ignoreVersion>
			</ignoreVersions>
		</rule>
	</rules>
</ruleset>
